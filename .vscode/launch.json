    "version": "0.2.0",
    "configurations": [
        {
            "name": "Simple Track Converation",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "simple_track_conversation.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
                "SLACK_SIGNING_SECRET": "",
                "SLACK_BOT_TOKEN": ""
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--port",
                "3000"
            ],
            "jinja": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "SLACK_SIGNING_SECRET": "",
                "SLACK_BOT_TOKEN": ""
            }
        },
        {
            "name": "Simple RTM",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "simple_RTM.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
                "SLACK_SIGNING_SECRET": "",
                "SLACK_BOT_TOKEN": ""
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--port",
                "3000"
            ],
            "jinja": true
        }
    ]
}