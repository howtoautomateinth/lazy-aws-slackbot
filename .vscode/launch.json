    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "RTM.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0",
                "SLACK_SIGNING_SECRET" : "secret here",
                "SLACK_BOT_TOKEN" : "token here"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--port",
                "3000"
            ],
            "jinja": true
        }
    ]
}